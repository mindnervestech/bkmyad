# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Controller
GET     /                                      controllers.Application.index()
GET 	/home.html							   controllers.Application.home()
GET     /Booknewspaperaddonlinee43a.html       controllers.Application.Booknewspaperaddonlinee43a()
GET 	/textClassifiedController.html		   controllers.TextClassifiedController.index()
GET		/sidebar.html						   controllers.SideBarController.index()
POST    /saveSignUp                            controllers.Application.SaveSignUp()
GET		/make-booking.html					   controllers.MakeBookingController.index()
GET		/getRatesByNewspaper/:newspaper/:Category	controllers.MakeBookingController.getRatesByNewspaper(newspaper:String,Category:String)
GET     /getBasicRateByLocationAndCategory/:Location/:Category   controllers.MakeBookingController.getBasicRateByLocationAndCategory(Location:String,Category:String)
GET     /checkusercreadientals/:Username/:Password               controllers.MakeBookingController.checkUserNameandPassword(Username:String,Password:String)
GET     /getcity/:statename						controllers.MakeBookingController.getcity(statename:String)
POST    /checkSignIn                           controllers.Application.CheckSignIn()  
POST    /savedispAddDetails                    controllers.Application.SavedispAddDetails()
GET     /addisplay.html                        controllers.AdDisplayController.index()    
POST    /validateUsernameUnique                controllers.UservalidationController.ValidateUsernameUnique()
GET     /dispFrame.html                        controllers.DispFrameController.index()
GET     /getSubCategoty/:cname					controllers.Application.getSubCategoty(cname:String)
POST    /submit-cart                            controllers.MakeBookingController.saveComposeyourAd()
#GET		/getAnnouncements/:title/:currentPage	controllers.AnnouncementController.getAnnouncements(title :String,currentPage :Integer)
POST	/saveAnnouncement		               controllers.AnnouncementController.saveAnnouncement
GET		/deleteAnnouncement/:id		           controllers.AnnouncementController.deleteAnnouncement(id :Long)
POST	/updateAnnouncement		            controllers.AnnouncementController.updateAnnouncement
GET		/getBasicrate/:City/:currentPage	controllers.AddbasicRateController.getBasicrate(City :String,currentPage :Integer)
POST	/saveBasicRate		               controllers.AddbasicRateController.saveBasicRate
POST	/updateBasicRate	            controllers.AddbasicRateController.updateBasicRate
GET		/deleteBasicRate/:id		           controllers.AddbasicRateController.deleteBasicRate(id :String)
GET     /getCategory							controllers.AddbasicRateController.getCategory
GET     /getStatename							controllers.AddbasicRateController.getStatename
GET     /getCityname/:state							controllers.AddbasicRateController.getCityname(state:String)

#GET    /addisplay.html                                 
                        
# Map static resources from the /public folder to the /assets URL path
GET /css/*file                     controllers.Assets.at(path="/public/app/css", file)
GET /lib/*file                     controllers.Assets.at(path="/public/lib", file)
GET /NewDesign/*file               controllers.Assets.at(path="/public/app/NewDesign", file)
GET /images/*file                  controllers.Assets.at(path="/public/app/images", file)
GET /image/*file                   controllers.Assets.at(path="/public/app/image", file)
GET /Adsimage/*file                 controllers.Assets.at(path="/public/app/Adsimage", file)
GET /Category/*file                 controllers.Assets.at(path="/public/app/Category", file)
GET /bower_components/*file		    controllers.Assets.at(path="/public/bower_components", file)
GET /newtheme/*file			     	controllers.Assets.at(path="/public/app/newtheme", file)
GET /assets/*file			    	controllers.Assets.at(path="/public/app/newtheme/assets", file)
GET /Jquery/*file			    	controllers.Assets.at(path="/public/app/Jquery", file)


#HTML - ONLY FOR STATIC HTMLs, IF HTMLS NEED TO BE TEMPLATIZED MAP it as Controllers
