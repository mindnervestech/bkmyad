# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Controller
GET     /                                   controllers.Application.index()
GET     /Booknewspaperaddonlinee43a.html    controllers.Application.Booknewspaperaddonlinee43a()
GET		/aboutUs							controllers.Application.AboutUs()
GET		/contactUs							controllers.Application.ContactUs()
GET		/ourClients		                    controllers.Application.OurClients()
GET		/ourBlogs		                    controllers.Application.OurBlogs()
GET     /termsandConditions                  controllers.Application.TermsandConditions()
GET     /privacyPolicy                        controllers.Application.PrivacyPolicy()
GET     /paymentOptions                        controllers.Application.PaymentOptions()
GET     /disClaimer                              controllers.Application.DisClaimer()
GET     /Help                             controllers.Application.Help()
        
GET     /dispAdd                             controllers.Application.DisplayAdd()
GET     /eNewsPaper                            controllers.Application.ENewsPaper()
GET     /affiliatedLogin                            controllers.Application.AffiliatedLogin()
GET     /registeredUser                      controllers.Application.RegisteredUser()
POST    /saveSignUp                              controllers.Application.SaveSignUp()
POST    /checkSignIn                             controllers.Application.CheckSignIn()
POST   /validateUsernameUnique                controllers.Application.ValidateUsernameUnique()
POST    /savedispAddDetails                   controllers.Application.SavedispAddDetails()

# Map static resources from the /public folder to the /assets URL path
GET /css/*file               controllers.Assets.at(path="/public/app/css", file)
GET /lib/*file               controllers.Assets.at(path="/public/lib", file)
GET /NewDesign/*file               controllers.Assets.at(path="/public/app/NewDesign", file)
GET /images/*file               controllers.Assets.at(path="/public/app/images", file)
GET /image/*file               controllers.Assets.at(path="/public/app/image", file)
GET /Adsimage/*file               controllers.Assets.at(path="/public/app/Adsimage", file)
GET /Category/*file               controllers.Assets.at(path="/public/app/Category", file)
GET /bower_components/*file		  controllers.Assets.at(path="/public/bower_components", file)


#HTML - ONLY FOR STATIC HTMLs, IF HTMLS NEED TO BE TEMPLATIZED MAP it as Controllers
GET /header.html     controllers.Assets.at(path="/public", file="app/header.html")
GET /footer.html     controllers.Assets.at(path="/public", file="app/footer.html")
GET /index.html     controllers.Assets.at(path="/public", file="app/index.html")
